{
  "_id": "rust_book",
  "startUrl": [
    "https://doc.rust-lang.org/book/title-page.html"
  ],
  "selectors": [
    {
      "delay": 0,
      "id": "next",
      "multiple": false,
      "parentSelectors": [
        "_root",
        "next"
      ],
      "selector": "a.next",
      "type": "SelectorLink"
    },
    {
      "delay": 0,
      "id": "page",
      "multiple": true,
      "parentSelectors": [
        "_root",
        "next"
      ],
      "selector": "main",
      "type": "SelectorElement"
    },
    {
      "delay": 0,
      "id": "title",
      "multiple": false,
      "parentSelectors": [
        "page"
      ],
      "regex": "",
      "selector": ":is(h1,h2)",
      "type": "SelectorText"
    }
  ]
}
