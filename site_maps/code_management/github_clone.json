{
  "_id": "github_repos_clone",
  "startUrl": [
    "https://github.com/MajorZiploc?tab=repositories"
  ],
  "selectors": [
    {
      "delay": 0,
      "id": "repo",
      "multiple": true,
      "parentSelectors": [
        "_root",
        "next_page"
      ],
      "selector": "li[itemprop='owns']",
      "type": "SelectorElement"
    },
    {
      "delay": 0,
      "id": "repo_name",
      "multiple": false,
      "parentSelectors": [
        "repo"
      ],
      "selector": "a[itemprop='name codeRepository']",
      "type": "SelectorLink"
    },
    {
      "clickElementSelector": "summary.btn-primary",
      "clickElementUniquenessType": "uniqueText",
      "clickType": "clickOnce",
      "delay": 2000,
      "discardInitialElements": "do-not-discard",
      "id": "code_btn",
      "multiple": false,
      "parentSelectors": [
        "repo_name"
      ],
      "selector": "tab-container div:nth-of-type(3) div.input-group",
      "type": "SelectorElementClick"
    },
    {
      "delay": 0,
      "extractAttribute": "value",
      "id": "clone",
      "multiple": false,
      "parentSelectors": [
        "code_btn"
      ],
      "selector": "input",
      "type": "SelectorElementAttribute"
    },
    {
      "delay": 0,
      "id": "next_page",
      "multiple": false,
      "parentSelectors": [
        "_root",
        "next_page"
      ],
      "selector": "a.btn-outline:contains('Next')",
      "type": "SelectorLink"
    }
  ]
}
