{
  "_id": "mangaowl_favorites",
  "startUrl": [
    "https://mangaowl.to/4-favorites"
  ],
  "selectors": [
    {
      "id": "card",
      "multiple": true,
      "parentSelectors": [
        "_root"
      ],
      "selector": ".manga-item a:nth-of-type(1)",
      "type": "SelectorLink"
    },
    {
      "id": "manga_page",
      "multiple": false,
      "parentSelectors": [
        "card"
      ],
      "selector": "div.content-container",
      "type": "SelectorElement"
    },
    {
      "id": "title",
      "multiple": false,
      "parentSelectors": [
        "manga_page"
      ],
      "regex": "",
      "selector": "h1",
      "type": "SelectorText"
    },
    {
      "id": "descr",
      "multiple": false,
      "parentSelectors": [
        "manga_page"
      ],
      "regex": "",
      "selector": "span.story-desc",
      "type": "SelectorText"
    },
    {
      "id": "status",
      "multiple": false,
      "parentSelectors": [
        "manga_page"
      ],
      "regex": "",
      "selector": "div.section-status:nth-of-type(2) span.default",
      "type": "SelectorText"
    },
    {
      "id": "author",
      "multiple": false,
      "parentSelectors": [
        "manga_page"
      ],
      "regex": "",
      "selector": "div.comic-attrs .column.my-2 span:contains(\"Author\") + div",
      "type": "SelectorText"
    },
    {
      "id": "genres",
      "multiple": false,
      "parentSelectors": [
        "manga_page"
      ],
      "regex": "",
      "selector": "div.comic-attrs .column.my-2 span:contains(\"Genres\") + div",
      "type": "SelectorText"
    }
  ]
}
